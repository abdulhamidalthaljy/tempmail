{
  "version": 2,
  "name": "tempmail-frontend",
  "builds": [
    {
      "src": "frontend/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/client/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd frontend/client && npm run build:vercel",
  "outputDirectory": "frontend/client/dist/client/browser",
  "installCommand": "cd frontend/client && npm install",
  "framework": "angular"
}
