{
  "name": "tempmail",
  "version": "1.0.0",
  "description": "A temporary email service similar to Mohmal and TempMail",
  "main": "backend/src/server.js",
  "scripts": {
    "start": "cd backend && node src/server.js",
    "build": "cd backend && npm install",
    "dev": "cd backend && npm run dev",
    "test": "cd backend && npm test",
    "install": "cd backend && npm install",
    "postinstall": "cd backend && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mongoose": "^8.0.0",
    "nodemailer": "^6.9.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.0.0",
    "dotenv": "^16.3.0"
  },
  "keywords": [
    "tempmail",
    "temporary-email",
    "disposable-email",
    "mailgun",
    "nodejs",
    "angular"
  ],
  "author": "abdulhamidalthaljy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/abdulhamidalthaljy/tempmail.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
